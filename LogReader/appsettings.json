{
  "ConnectionStrings": {
    //"LogDatabase": "Data Source=C:\\Temp\\logging.db"
    "LogDatabase": "Data Source=Inmemory;Mode=Memory;Cache=Shared"
  },

  "Settings": {
    "StateDictionary": {
      "file-loaded": {
        "IsProgress": true,
        "StatusText": "Getting loaded data details..."
      },
      "file-error": {
        "IsError": true,
        "StatusText": "File error",
        "PopupText": "Unable to read the file. Please check if the file still exist and try again"
      },
      "file-small": {
        "IsError": true,
        "StatusText": "File error",
        "PopupText": "File is too small. Check the file and try again."
      },
      "parse-error": {
        "IsError": true,
        "StatusText": "Parsing error",
        "PopupText": "Unable to parse the file. Please check if it's in the Syngo log format."
      },
      "empty-xml": {
        "IsError": true,
        "StatusText": "Empty XML file",
        "PopupText": "The file is in correct XML format but has no data."
      },
      "parse-ok": {
        "StatusText": "Parsing finished"
      },
      "save-sql": {
        "IsProgress": true,
        "StatusText": "Parsing finished. Saving data to the database..."
      },
      "file-cancel": {
        "IsError": true
      },
      "archive-error": {
        "IsError": true,
        "StatusText": "Archive error",
        "PopupText": "File was recognized as an archive but decompression failed. Try to decompress the file manually."
      },
      "archive-gz": {
        "IsProgress": true,
        "StatusText": "decompressing GZip file..."
      },
      "archive-zip": {
        "IsProgress": true,
        "StatusText": "decompressing ZIP file..."
      },
      "parse-xml": {
        "IsProgress": true,
        "StatusText": "Parsing XML file..."
      },
      "dirty-xml": {
        "IsProgress": true,
        "StatusText": "Attempt to sanitize XML file..."
      },
      "parse-txt": {
        "IsProgress": true,
        "StatusText": "Parsing TXT file..."
      },
      "query-run": {
        "IsProgress": true,
        "StatusText": "Query running..."
      },
      "empty": {
        "StatusText": "Database is empty"
      },
      "result": {
        "StatusText": ""
      },
      "save-ok": {
        "StatusText": "File saved",
        "PopupText": "Export to XML was succefully created and saved to the file."
      },
      "save-error": {
        "IsError": true,
        "StatusText": "File was not saved",
        "PopupText": "Export to XML failed."
      }
    }
  }
}